{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "managedDependency": {
    "enabled": true
  },
  "functionTimeout": "-1",
  "extensions": {
    "queues": {
        "maxPollingInterval": "00:01:00",
        "visibilityTimeout" : "00:30:00",
        "batchSize": 2,
        "maxDequeueCount": 5,
        "newBatchThreshold": 1,
        "messageEncoding": "base64"
    }
}  
}
